<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.sandag</groupId>
	<artifactId>SANDAG-ABM</artifactId>
	<version>0.0.1-SNAPSHOT</version>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-checkstyle-plugin</artifactId>
				<version>2.10</version>
				<configuration>
          <failsOnError>false</failsOnError>
					<configLocation>checkstyle.xml</configLocation>
				</configuration>
			</plugin>
		</plugins>
	</reporting>
 
	<dependencies>
		<dependency>
			<groupId>pb</groupId>
			<artifactId>common-base</artifactId>
			<version>1.0-ABM</version>
		</dependency>
		<dependency>
			<groupId>pb</groupId>
			<artifactId>sawdust</artifactId>
			<version>unknown</version> 
		</dependency>
		<dependency>
			<groupId>org.jppf</groupId>
			<artifactId>jppf-client</artifactId>
			<version>2.5</version>
		</dependency>
		<dependency>
			<groupId>org.jppf</groupId>
			<artifactId>jppf-server</artifactId>
			<version>2.5</version>
		</dependency>
		<dependency>
			<groupId>ca.umontreal.iro</groupId>
			<artifactId>ssj</artifactId>
			<version>2.5</version>
			<exclusions>
				<exclusion>
					<artifactId>icu4j</artifactId>
					<groupId>com.ibm.icu</groupId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>commons-lang</groupId>
			<artifactId>commons-lang</artifactId>
			<version>2.4</version>
		</dependency>
	</dependencies>

	<distributionManagement>
		<snapshotRepository>
			<id>nexus-snapshots</id>
			<name>Snapshots</name>
			<url>http://svn.sandag.org:8081/nexus/content/repositories/snapshots</url>
			<uniqueVersion>false</uniqueVersion>
		</snapshotRepository>
		<repository>
			<id>nexus-release</id>
			<name>Releases</name>
		<url>http://svn.sandag.org:8081/nexus/content/repositories/releases/</url>
		</repository>
	</distributionManagement>
  
	<repositories>
		<repository>
			<id>nexus-thirdparty</id>
			<name>Sandag Nexus</name>
			<url>http://svn.sandag.org:8081/nexus/content/repositories/thirdparty</url>
		</repository>
		<repository>
			<id>nexus-release</id>
			<name>Sandag Nexus Release</name>
			<url>http://svn.sandag.org:8081/nexus/content/repositories/release</url>
		</repository>
		<repository>
			<id>nexus-snapshots</id>
			<name>Sandag Nexus Snapshots</name>
			<url>http://svn.sandag.org:8081/nexus/content/repositories/snapshots</url>
		</repository>
	</repositories>
</project>
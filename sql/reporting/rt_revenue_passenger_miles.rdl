  <DataSets>
    <DataSet Name="route_operations">
      <Query>
        <DataSourceName>rtp_sql2014a8_abm_13_2_3</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@scenario_id">
            <Value>=Parameters!scenario_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@route_id">
            <Value>=Parameters!route_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH ops as
(
SELECT
  flow.scenario_id
  ,rt.config / 1000 as rt
  ,rt.config % 100 as conf
  ,CASE (rt.config % 1000) / 100
    WHEN 1 THEN 'Inbound'
    WHEN 2 THEN 'Outbound'
  END as dir
  ,p.time_period_id
  ,CASE p.time_period_id
    WHEN 2 THEN 'Early AM'
	WHEN 3 THEN 'AM Peak'
	WHEN 4 THEN 'Midday'
	WHEN 5 THEN 'PM Peak'
	WHEN 6 THEN 'Night'
  END as time_period
  ,sum(flow.transit_flow * (flow.to_mp - flow.from_mp)) as passenger_miles
  ,(rt.shape.STLength() / 1609.34) *
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END as revenue_miles
  ,(rt.shape.STLength() / 1609.34) *
  CASE 
    WHEN rt.transit_mode_id = 4 THEN 130*5 --COASTER and Heavy Rail
	WHEN rt.transit_mode_id = 5 AND rt.config / 1000 &gt; 500 AND rt.config / 1000 &lt; 551 THEN 64 * 3 --TROLLEY
	WHEN rt.transit_mode_id = 5 AND rt.config / 1000 = 399 THEN 136 * 2 --TROLLEY
	WHEN rt.transit_mode_id = 5 AND ((rt.config / 1000 &gt;= 551 AND rt.config / 1000 &lt;= 559) OR rt.config / 1000 = 565) THEN 37 --streetcar
	WHEN rt.transit_mode_id in (8,6) THEN 53 --BRT
	WHEN rt.transit_mode_id in (7,9,10) THEN 37 --BUS
	ELSE 0
  END *
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END as seat_miles
  ,CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END as vehicles
  ,ivt.ivt * CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END as link_travel_time
  ,CASE 
    WHEN rt.transit_mode_id = 10 THEN (stops.stops - 1) * 0.3 
    WHEN rt.transit_mode_id BETWEEN 6 AND 9 THEN (stops.stops - 1) * 0.5
    ELSE 0 
  END *
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END dwell_time
FROM
  abm.transit_flow flow
  JOIN abm.transit_route rt ON flow.scenario_id = rt.scenario_id AND flow.transit_route_id = rt.transit_route_id
  JOIN ref.time_period p ON flow.time_period_id = p.time_period_id
  JOIN (SELECT scenario_id, transit_route_id, COUNT(*) stops FROM abm.transit_stop GROUP BY scenario_id, transit_route_id) stops ON rt.scenario_id = stops.scenario_id and rt.transit_route_id = stops.transit_route_id
  JOIN (SELECT scenario_id,transit_route_id,time_period_id,sum(avg_ivt_by_mode_access) ivt FROM (SELECT scenario_id,transit_route_id,from_transit_stop_id ,to_transit_stop_id ,time_period_id ,sum(baseivtt) / count(*) as avg_ivt_by_mode_access FROM abm.transit_flow GROUP BY scenario_id,transit_route_id,from_transit_stop_id,to_transit_stop_id,time_period_id) ivt GROUP BY scenario_id,transit_route_id,time_period_id) ivt ON flow.scenario_id = ivt.scenario_id and flow.transit_route_id = ivt.transit_route_id and flow.time_period_id = ivt.time_period_id
GROUP BY
  flow.scenario_id
  ,rt.config / 1000
  ,rt.config %100
  ,(rt.config % 1000) / 100 
  ,p.time_period_id
  ,CASE p.time_period_id
    WHEN 2 THEN 'Early AM'
	WHEN 3 THEN 'AM Peak'
	WHEN 4 THEN 'Midday'
	WHEN 5 THEN 'PM Peak'
	WHEN 6 THEN 'Night'
  END
  ,(rt.shape.STLength() / 1609.34) *
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END
  ,(rt.shape.STLength() / 1609.34) *
  CASE 
    WHEN rt.transit_mode_id = 4 THEN 130*5 --COASTER and Heavy Rail
	WHEN rt.transit_mode_id = 5 AND rt.config / 1000 &gt; 500 AND rt.config / 1000 &lt; 551 THEN 64 * 3 --TROLLEY
	WHEN rt.transit_mode_id = 5 AND rt.config / 1000 = 399 THEN 136 * 2 --TROLLEY
	WHEN rt.transit_mode_id = 5 AND ((rt.config / 1000 &gt;= 551 AND rt.config / 1000 &lt;= 559) OR rt.config / 1000 = 565) THEN 37 --streetcar
	WHEN rt.transit_mode_id in (8,6) THEN 53 --BRT
	WHEN rt.transit_mode_id in (7,9,10) THEN 37 --BUS
	ELSE 0
  END *
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END,
  ivt.ivt * CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END,
  CASE 
    WHEN rt.transit_mode_id = 10 THEN (stops.stops - 1) * 0.3 
    WHEN rt.transit_mode_id BETWEEN 6 AND 9 THEN (stops.stops - 1) * 0.5
    ELSE 0 
  END *
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END,
  CASE p.time_period_id
    WHEN 2 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --EARLY AM
	WHEN 3 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.am_headway --AM PEAK
	WHEN 4 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.op_headway --MIDDAY 
	WHEN 5 THEN (DATEDIFF(mi, p.time_period_start,p.time_period_end) + 1) / rt.pm_headway
	WHEN 6 THEN (rt.nt_hour * 60) / NULLIF(rt.nt_headway,0) --EVENING / NIGHTTIME
  END),
oo as (
SELECT 
  oo.scenario_id
  ,rt.config / 1000 as rt
  ,rt.config % 100 as conf
  ,CASE (rt.config % 1000) / 100
    WHEN 1 THEN 'Inbound'
	WHEN 2 THEN 'Outbound'
  END as dir
  ,oo.time_period_id
  ,SUM(oo.boardings) as boardings
  ,SUM(oo.alightings) as alightings
FROM
  abm.transit_onoff oo
  JOIN abm.transit_route rt ON oo.scenario_id = rt.scenario_id AND oo.transit_route_id = rt.transit_route_id
GROUP BY 
  oo.scenario_id
  ,rt.config / 1000
  ,rt.config % 100
  ,CASE (rt.config % 1000) / 100
    WHEN 1 THEN 'Inbound'
	WHEN 2 THEN 'Outbound'
  END
  ,oo.time_period_id)

SELECT
  ops.scenario_id
  ,ops.rt
  ,ops.dir
  ,ops.conf
  ,ops.time_period_id
  ,ops.time_period
  ,oo.boardings
  ,oo.alightings
  ,ops.passenger_miles
  ,ops.revenue_miles
  ,ops.seat_miles
  ,ops.link_travel_time + ops.dwell_time as revenue_minutes
  ,(ops.link_travel_time + ops.dwell_time) / vehicles rt_travel_time
  ,ops.revenue_miles / ((ops.link_travel_time + ops.dwell_time) / 60) as avg_speed
  ,vehicles
FROM
  ops
  JOIN oo ON ops.scenario_id = oo.scenario_id AND ops.rt = oo.rt AND ops.conf = oo.conf AND ops.dir = oo.dir AND ops.time_period_id = oo.time_period_id
WHERE
  ops.scenario_id = @scenario_id
  and ops.rt = @route_id</CommandText>
